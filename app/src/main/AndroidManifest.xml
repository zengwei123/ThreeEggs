<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zengwei.threeeggs">
    <!--网络请求权限-->
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Guide.GuideActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main.MainActivity"></activity>
        <activity android:name="com.example.z_order.orderPage.OrderPageActivity"></activity>
        <activity android:name="com.example.z_home.Category.CategoryActivity"></activity>
        <activity android:name="com.example.z_home.Address.AddressActivity"></activity>
    </application>
</manifest>